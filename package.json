{
  "name": "gravity",
  "version": "0.2.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "electron": "electron .",
    "lint": "ng lint",
    "ng": "ng",
    "serve": "ng serve",
    "start": "ng build && electron .",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~7.2.2",
    "@angular/cdk": "~7.3.0",
    "@angular/common": "~7.2.2",
    "@angular/compiler": "~7.2.2",
    "@angular/core": "~7.2.2",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.2",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "~7.2.2",
    "@angular/platform-browser-dynamic": "~7.2.2",
    "@angular/router": "~7.2.2",
    "core-js": "^2.6.3",
    "drag-drop": "^4.2.0",
    "electron-reload": "^1.4.0",
    "electron-store": "^2.0.0",
    "ffmpeg-static": "^2.4.0",
    "file-type": "^10.7.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.1",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "nanoid": "^2.0.1",
    "nouislider": "^12.1.0",
    "read-chunk": "^3.0.0",
    "rxjs": "~6.3.3",
    "sanitize-filename": "^1.6.1",
    "sortablejs": "^1.8.1",
    "tslib": "^1.9.3",
    "ytdl-core": "^0.29.1",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.3",
    "@angular/cli": "~7.2.3",
    "@angular/compiler-cli": "~7.2.2",
    "@angular/language-service": "~7.2.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.19",
    "app-root-path": "^2.1.0",
    "codelyzer": "~4.5.0",
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "eslint": "^5.12.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "mocha": "^5.2.0",
    "ngx-electron": "^2.1.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4"
  },
  "build": {
    "appId": "com.iohzrd.gravity",
    "asarUnpack": "**/node_modules/ffmpeg-binaries/**/*",
    "publish": [
      {
        "provider": "generic",
        "url": "https://example.com/updates"
      }
    ],
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "perMachine": true
    }
  },
  "productName": "gravity"
}
